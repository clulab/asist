{
	"definitions": {
	},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://github.com/clulab/tomcat-text/message_specs/data/extraction/extraction.json",
	"type": "object",
	"title": "data schema",
	"version": "0.5",
	"required": [
		"label",
		"span",
		"arguments",
		"taxonomy_matches"
       	],
	"properties": {
		"label": {
			"$id": "#/properties/label",
			"type": "string",
			"title": "label schema",
			"default": "",
			"examples": [ "Sight" ],
			"pattern": "^(.*)$"
		},
		"span": {
			"$id": "#/properties/span",
			"type": "string",
			"title": "span schema",
			"default": "",
			"examples": [ "was one yellow victim" ],
			"pattern": "^(.*)$"
		},
		"arguments": {
			"$id": "#/properties/arguments",
			"type": "string",
			"title": "arguments schema",
			"default": "",
			"examples": [ "Victim" ],
			"pattern": "^(.*)$"
		},
		"taxonomy_matches": {
			"$id": "#/properties/taxonomy_matches",
			"type": "Array[TaxonomyMatch]",
			"title": "taxonomy_matches schema",
			"default": "",
			"examples": [ [ {"finish-triaging": "7.5568876562069995"},{"start-triaging": "7.3469120111747985"}]],
			"pattern": "^(.*)$"
		}
	}
}

